<div id="<%= dom_id event %>">
  <p>
    <strong>Subject:</strong>
    <%= event.subject %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= event.body %>
  </p>

  <p>
    <strong>Strayed:</strong>
    <%= event.strayed_id %>
  </p>

  <p>
    <strong>Rescuer:</strong>
    <%= event.rescuer_id %>
  </p>

  <p>
    <strong>State:</strong>
    <%= event.state %>
  </p>

  <p>
    <strong>Gps:</strong>
    <%= event.gps %>
  </p>

</div>
